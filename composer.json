{
    "name": "neuron-core/neuron-laravel",
    "description": "Official Neuron AI Laravel SDK.",
    "keywords": [
        "ai",
        "ai-agent",
        "llm",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Valerio Barbera",
            "email": "valerio@inspector.dev"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/neuron-core/neuron-ai"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "neuron-core/neuron-ai": "3.x-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "orchestra/testbench": "^10.0",
        "livewire/livewire": "^3.7",
        "larastan/larastan": "^3.0",
        "rector/rector": "^2.2",
        "tomasvotruba/type-coverage": "^2.0"
    },
    "suggest": {
        "inspector-apm/inspector-laravel": "^4.18.2"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NeuronAI\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NeuronAI\\Laravel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always"
        ],
        "format": [
            "php-cs-fixer fix --allow-risky=yes",
            "vendor/bin/rector"
        ],
        "analyse": [
            "vendor/bin/phpstan analyse --memory-limit=2G -v"
        ],
        "style": [
            "php-cs-fixer fix --allow-risky=yes"
        ],
        "refactor": [
            "vendor/bin/rector"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NeuronAI\\Laravel\\NeuronAIServiceProvider"
            ],
            "aliases": {
                "AIProvider": "NeuronAI\\Laravel\\Facades\\AIProvider",
                "EmbeddingProvider": "NeuronAI\\Laravel\\Facades\\EmbeddingProvider",
                "VectorStore": "NeuronAI\\Laravel\\Facades\\VectorStore"
            }
        }
    }
}
